{
  "name": "@fiatdao/sdk",
  "version": "0.0.34",
  "description": "Javascript library for interacting with the FIAT protocol's contracts",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./lib && tsc && babel ./src -d ./lib",
    "bump": "yarn upgrade changelog",
    "ci": "yarn build && jest",
    "test": "yarn build && source ./.env && jest"
  },
  "files": [
    "lib/",
    "src/",
    "README.md",
    "yarn.lock"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "chrome >= 66",
              "firefox >= 68"
            ]
          }
        }
      ]
    ],
    "sourceMaps": "inline"
  },
  "dependencies": {
    "changelog": "https://github.com/fiatdao/changelog",
    "ethers": "^5.7.1",
    "ethers-multicall": "^0.2.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "ganache": "^7.4.4",
    "jest": "^29.1.2",
    "typescript": "4.8.4"
  }
}
